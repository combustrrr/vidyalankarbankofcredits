{
  "name": "vidyalankarbankofcredits",
  "version": "1.0.0",
  "description": "A full-stack web application for Vidyalankar Bank of Credits with Next.js, Tailwind CSS, and Express backend.",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "next dev",
    "dev:server": "cross-env SERVER_PORT=4000 ts-node --project tsconfig.server.json server/index.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production concurrently \"next start\" \"cross-env SERVER_PORT=4000 node dist/server/index.js\"",
    "start:server": "cross-env SERVER_PORT=4000 node dist/server/index.js",
    "start:client": "next start",
    "prepare": "mkdir -p node_modules",
    "migrate:db": "node utils/database/migration-manager.js",
    "migrate:db:force": "node utils/database/migration-manager.js --force",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "cleanup-ports": "node utils/kill-debug-ports.js",
    "kill:ports": "node utils/kill-debug-ports.js",
    "setup:db": "npm run migrate:db",
    "start:all": "npm run kill:ports && npm run setup:db && npm run dev",
    "quick-start": "node start-app.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "@types/jsonwebtoken": "^9.0.9",
    "axios": "^1.5.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "next": "^13.4.19",
    "pg": "^8.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.2.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.0",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.4.19",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
